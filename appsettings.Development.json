{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "UrlValidationService": "Debug"
    }
  },
  
  "Validation": {
    "SyncThreshold": 5,
    "RequestTimeoutSeconds": 5,
    "MaxConcurrency": 5,
    "BatchSize": 10,
    "WorkerPollingIntervalSeconds": 2
  },
  
  "Cache": {
    "TtlMinutes": 5
  }
}
